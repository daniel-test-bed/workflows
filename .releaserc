{
  "tagFormat": "v${version}",
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    ["@semantic-release/git", {
      "assets": [],
      "message": "signed release",
      "sign": true
    }],
    ["@semantic-release/exec", {
      "prepareCmd": "echo 'Preparing release ${nextRelease.version}'",
      "publishCmd": "echo 'Publishing release ${nextRelease.version}'",
      "successCmd": "gpg --batch --yes --armor --detach-sign --passphrase '${GPG_PASSPHRASE}' -u '${GPG_KEY_ID}' 'dist/*'"
    }]
  ]
}
